{
  "name": "getter_weather",
  "version": "1.0.0",
  "description": "It'll be less weather with getter weather!",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juhuyoon/getter_weather.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/juhuyoon/getter_weather/issues"
  },
  "homepage": "https://github.com/juhuyoon/getter_weather#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "mongoose": "^5.6.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1",
    "reactstrap": "^8.0.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "concurrently": "^4.1.1",
    "mocha": "^6.1.4"
  }
}
